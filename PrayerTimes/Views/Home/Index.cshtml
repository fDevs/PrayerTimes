@model IEnumerable<PrayerTimes.Models.Masjid>
@{
    ViewBag.Title = "Home Page";
}
<br>
<h1 align="center">Jamat Timings</h1>
<table class="table-hover table table-bordered">
    <tr>
        <th>
            ID
        </th>
        <th>
            Name
        </th>
        <th>
            Fajr
        </th>
        <th>
            Zhur
        </th>
        <th>
            Asr
        </th>
        <th>
            Magib
        </th>
        <th>
            Isha
        </th>
        <th>
            Jumah 1
        </th>
        <th>
            Jumah 2
        </th>
        
    </tr>
    
        @foreach (var masjid in Model)
        {
            <tr>
                <th>
                    @masjid.MasjidID
                </th>
                <th>
                    @masjid.Name
                </th>
                <th>
                    @masjid.prayerTimings.Fajr.Hour:@masjid.prayerTimings.Fajr.Minute
                </th>
                <th>
                    @masjid.prayerTimings.Zhur.Hour:@masjid.prayerTimings.Zhur.Minute
                </th>
                <th>
                    @masjid.prayerTimings.Zhur.Hour:@masjid.prayerTimings.Zhur.Minute
                </th>
                <th>
                    @masjid.prayerTimings.Zhur.Hour:@masjid.prayerTimings.Zhur.Minute
                </th>
                <th>
                    @masjid.prayerTimings.Zhur.Hour:@masjid.prayerTimings.Zhur.Minute
                </th>
                <th>
                    @masjid.prayerTimings.Zhur.Hour:@masjid.prayerTimings.Zhur.Minute
                </th>
                <th>
                    @masjid.prayerTimings.Zhur.Hour:@masjid.prayerTimings.Zhur.Minute
                </th>
            </tr>
        }
            
</table>